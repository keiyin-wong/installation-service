<?xml version="1.0" encoding="UTF-8"?>

<configuration status="INFO">
    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} [%p] %c - %m%n" />
        </Console>
    </appenders>
    <loggers>
        <root level="INFO">
            <appender-ref ref="Console" />
        </root>
    </loggers>
</configuration>